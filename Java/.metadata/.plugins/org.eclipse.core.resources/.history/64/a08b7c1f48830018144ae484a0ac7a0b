package com.hpe.java;

public class BreakTest {

	public static void main(String[] args) {

		test1();
		
	}
	
	//break：跳出当前整个循环
	public static void test1(){
		
		for(int i = 0; i <3; i++){
			
			for(int j = 0; j < 3; j++){
				
				if(j == 1)
					break;
				
				System.out.println("i:" + i + "j:" + j);
				
			}
			
		}
		
	}
	

}
