package hpe.service;

import hpe.po.User;

/**
 * TODO user业务的接口
 * @author chaoling 
 * 2018年8月27日
 */
public interface IUserService {

	/**
	 * 根据id查询用户记录
	 * @param id 待查询用户的id
	 * @return	符合id的记录对应的实体类对象
	 */
	User selectUserById(int id);
	
	/**
	 * 根据username查询用户记录
	 * @param username 待查询用户的username
	 * @return	符合username的记录对应的实体类对象
	 */
	User selectUserByUsername(String username);

}
