<mapper namespace="test">
	<!-- 查询所有用户
		id:mapped statment 调用时需要，唯一
		resultType:结果集类型()
	 -->
	<select id="queryAllUser" resultMap="hpe.po.User">
		select * from user
	</select>

	<select id="queryAllUser" resultMap="hpe.po.User">
		select * from user
	</select>
	
</mapper>