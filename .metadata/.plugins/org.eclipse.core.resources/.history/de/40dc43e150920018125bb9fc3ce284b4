package com.hpe.java;

import com.hpe.java2.Bike;
import com.hpe.java2.Bus;
import com.hpe.java2.Vehicle;

/**
 * 
 * @author chaoling
 * @date 2018年7月17日下午7:11:03
 * @Description
 */
public class InterfaceDemo {
	public static void main(String[] args) {
		Vehicle bike = new Bike();
		bike.start();
		bike.stop();
		Vehicle bus = new Bus();
		bus.start();
		bus.stop();
	}

}
